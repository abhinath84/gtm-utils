{
  "source": {
    "include": ["./app/cli"],
    "exclude": [
    ],
    "includePattern": ".js$"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true,
    "default": {
      "staticFiles": {
        "paths": ["./jsdoc/static"]
      }
    }
  },
  "opts": {
    "recurse": true,
    "destination": "./app/doc",
    "template": "node_modules/minami",
    "tutorials": "./jsdoc/tutorials",
    "readme": "./jsdoc/readme.md"
  }
}
